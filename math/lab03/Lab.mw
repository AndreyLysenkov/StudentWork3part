</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L3" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion0" visible="true" pixel-width="450" pixel-height="430" code-language="text/maple" autoexpand="false" wrapping="false" show-border="true" code-line-numbers="true">LENGTH := 50:

randomizer := proc (min, max) :: integer:
	return min + (rand() mod (max - min + 1)):
end proc:

checkIndex := proc (i) :: boolean:
	return (i &gt; 0) and (i &lt;= LENGTH):
end proc:

LinalSearch := proc (A, value) :: integer:
local i:
	i := 1;
	for i from 1 to LENGTH do
		if (A[i] = value) then
			return i:
		end if:
	end do:
	return 0:
end proc:

LinalSearchRecursion := proc (A, i, j, value) :: integer:
	if not (checkIndex(i) and checkIndex(j)) then
		return 0:
	end if:
	if (i &lt;= j) and (A[i] &lt;&gt; value) then
		return LinalSearchRecursion(A, i + 1, j, value):
	end if:
	if (A[i] &lt;&gt; value) then
		i := 0:
	end if:
	return i:
end proc:

BinarySearch := proc (A, value) :: integer:
local left, right, i:
left := 1:
right := nops(A):
i := 1:
while (left &lt;= right) and (A[i] &lt;&gt; value) do
	i := iquo((left + right), 2):
	if not(checkIndex(i)) then
		return 0:
	end if:
	if (value &lt; A[i]) then
		right := i - 1:
	else
		left := i + 1:
	end if:
end do:
if (value = A[i]) then
	return i:
end if:
return 0:
end proc:

BinarySearchRecursion := proc (A, left, right, value) :: integer:
local i:
	i := iquo((left + right), 2):	
	if (left &gt; right) or (A[i] = value) then
		return i:
	end if:
	if not (checkIndex(left) and checkIndex(right)) then
		return 0:
	end if:
	if (value &lt; A[i]) then
		return BinarySearchRecursion(A, left, i - 1, value):
	end if:
	return BinarySearchRecursion(A, i + 1, right, value):
end proc:

A := [seq(randomizer(1, 100),i=1..50)];
B := sort(A);

x := readstat();

print (&quot;Linal Search &quot;, LinalSearch(A, x));
print (&quot;Linal Search Recursion: &quot;, LinalSearchRecursion(A, 1, 50, x));

print (&quot;Binary Search &quot;, BinarySearch(B, x));
print (&quot;Binary Search Recursion&quot;, BinarySearchRecursion(B, 1, 50, x));

randomizer(0, 1);</EC-CodeEditor>
